<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="600" prefWidth="800.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.MainMenu">
   <center>
        <VBox alignment="CENTER" prefHeight="538.0" prefWidth="718.0" stylesheets="@../css/style.css" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="profileMenu" maxWidth="354.0" mnemonicParsing="false" onMouseClicked="#enterProfileMenu" prefHeight="28.0" prefWidth="500.0" text="Profile Menu" />
                <Button fx:id="changeEnvironmentMenu" maxWidth="418.0" mnemonicParsing="false" onMouseClicked="#enterChangeEnvironmentMenu" prefHeight="28.0" prefWidth="500.0" text="Change Environment" />
                <Button fx:id="help" maxWidth="418.0" mnemonicParsing="false" onMouseClicked="#help" prefHeight="28.0" prefWidth="500.0" text="Help" />
                <Button fx:id="back" mnemonicParsing="false" onMouseClicked="#back" prefHeight="28.0" prefWidth="317.0" text="Back" />

            </children>
        </VBox>
   </center>
</BorderPane>
