<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.paint.LinearGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.control.TextField?>
<Pane xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="view.MapTransfer"
      prefHeight="400.0" prefWidth="600.0"
      stylesheets="@../css/style.css">
    <VBox alignment="CENTER" layoutX="380.0" layoutY="24.0" prefHeight="775.0" prefWidth="818.0" spacing="25.0" stylesheets="@../css/style.css">
        <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Stronghold Crusader" textAlignment="CENTER" wrappingWidth="495.9159622192383">
                <font>
                    <Font size="83.0" />
                </font>
                <fill>
                    <LinearGradient cycleMethod="REPEAT" endX="1.0" endY="1.0">
                        <stops>
                            <Stop>
                                <color>
                                    <Color />
                                </color>
                            </Stop>
                            <Stop offset="1.0">
                                <color>
                                    <Color red="1.0" green="0.11851851642131805" blue="0.11851851642131805" />
                                </color>
                            </Stop>
                        </stops>
                    </LinearGradient>
                </fill>
            </Text>
            <TextField fx:id="mapName" maxWidth="354.0" promptText="map name" />
            <TextField fx:id="reciever" maxWidth="354.0" promptText="reciever" />
            <Button fx:id="save" maxWidth="354.0" mnemonicParsing="false" onMouseClicked="#save" prefHeight="50.0" prefWidth="300.0" text="Save" />
            <Button fx:id="send" maxWidth="418.0" mnemonicParsing="false" onMouseClicked="#send" prefHeight="51.0" prefWidth="300.0" text="Send" />
            <Button fx:id="back" mnemonicParsing="false" onMouseClicked="#back" prefHeight="50.0" prefWidth="300.0" text="Back" />
        </children>
    </VBox>
</Pane>
